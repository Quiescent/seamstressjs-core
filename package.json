  "engines": {
    "node": ">= 6.10.2",
    "npm": ">= 3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Quiescent/seamstressjs-core.git"
  },
  "author": "",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Quiescent/seamstressjs-core/issues"
  },
  "homepage": "https://github.com/Quiescent/seamstressjs-core#readme",
  "devDependencies": {
    "chai": "3.5.0",
    "jscs": "3.0.7",
    "madge": "1.6.0",
    "mocha": "3.2.0",
    "mocha-testcheck": "1.0.0-rc.0",
    "next-update": "1.5.1",
    "nyc": "10.2.0",
    "testcheck": "1.0.0-rc.2",
    "webpack": "2.5.1"
  },
  "dependencies": {
    "ramda": "0.23.0"
